                 [:tab :!Stab] [:delete_or_backspace :!Sdelete_or_backspace]
                 ;; Arrows
                 [:up_arrow :!Sup_arrow] [:down_arrow :!Sdown_arrow]
                 [:left_arrow :!Sleft_arrow] [:right_arrow :!Sright_arrow]
                 ;;
                 ;; ]}

        {;;
         :des "spacebar-mode for navigation"
         :condi :!voyager
         :rules [;;
                 :condi :!voyager
                 :spacebar-mode
                 [:##h :left_arrow]
                 [:##j :down_arrow]
                 [:##k :up_arrow]
                 [:##l :right_arrow]
                 ;;
                 ]}

