<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTD/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.startup-maintenance</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/hs</string>
        <string>-c</string>
        <string>taskRunner.startupMaintenance()</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/startup-maintenance.out</string>

    <key>StandardErrorPath</key>
    <string>/tmp/startup-maintenance.err</string>
</dict>
</plist>
